
import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int [] arr={ 1,2,3,4,5,6,7,9,10};
		
		 int N= arr.length+1;
		 int final_sum=0;
		 int sum=0,dffrce=0;
		 final_sum = N*(N+1)/2;
		 for(int i=0;i<N-1;i++)
		 {
		     sum+=arr[i];
		 }
		dffrce= final_sum-sum;
		System.out.println(dffrce);

}}
